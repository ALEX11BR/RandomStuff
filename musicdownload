yt-dlp -x -o '%(artist)s-%(title)s.%(ext)s' --embed-metadata --parse-metadata 'channel:%(artist)s' -- 'https://global-link'
yt-dlp -x -o '%(artist)s-%(title)s.%(ext)s' --embed-metadata --parse-metadata 'release_year:(?s)(?P<meta_date>.+)' -- 'https://good-link'
yt-dlp -x -o '%(artist)s-%(title)s.%(ext)s' --embed-metadata --parse-metadata 'title:%(artist)s - %(title)s' -- 'https://video-link'
